{
  "workbench.colorTheme": "Dracula Theme",
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontSize": 14,
  "editor.lineHeight": 18,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.rulers": [80, 120],
  "editor.renderLineHighlight": "gutter",
  "editor.wordWrap": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": false,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.windows": "zsh",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "python.venvPath": "${workspaceFolder}/.venv",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.analysis.autoImportCompletions": true,
  "black-formatter.args": ["--line-length", "79"],
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnType": true
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "todohighlight.include": [
    "**/*.py",
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ],
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "editor.inlineSuggest.showToolbar": "always",
  "aws.suppressPrompts": {
    "codeWhispererNewWelcomeMessage": true
  },
  "redhat.telemetry.enabled": true,
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "remote.SSH.connectTimeout": 120,
  "remote.SSH.defaultExtensions": ["amazonwebservices.aws-toolkit-vscode"],
  "amazonQ.suppressPrompts": {
    "amazonQChatDisclaimer": true,
    "amazonQChatPairProgramming": true
  },
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "kiroAgent.agentModelSelection": "CLAUDE_SONNET_4_20250514_V1_0",
  "kiroAgent.agentAutonomy": "Supervised",
  "kiroAgent.trustedCommands": ["terraform *"],
  "kiroAgent.modelSelection": "claude-opus-4.5",
  "aws.cloudformation.telemetry.enabled": true,
  "codesnap.transparentBackground": true,
  "codesnap.boxShadow": "rgba(0, 0, 0, 0) 0px 0px 0px"
}
